<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="fich">
        <createTable tableName="FICH_TIENDAS">
            <column name="ID" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_FICH_TIENDAS"/>
            </column>
            <column name="TIENDA" type="VARCHAR(255)"/>
            <column name="NOMBRE_TIENDA" type="VARCHAR(255)"/>
            <column name="TIPO" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>